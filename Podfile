# platform :ios, '9.0'

target 'SmartListIOS' do
  # Comment the next line if you're not using Swift and don't want to use dynamic frameworks
  use_frameworks!

  # Pods for SmartListIOS
    pod 'Firebase/Core'
    pod 'FirebaseUI'
    
    pod 'FirebaseUI/Auth'
    
    pod 'FirebaseUI/Google'
    pod 'FirebaseUI/Facebook'
    pod 'FirebaseUI/Twitter'
    pod 'FirebaseUI/Phone'
    
  target 'SmartListIOSTests' do
    inherit! :search_paths
    # Pods for testing
  end

  target 'SmartListIOSUITests' do
    inherit! :search_paths
    # Pods for testing
  end

end
